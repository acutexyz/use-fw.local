			out::writeTo("specific", "content", array('name' => 'val'));
			/*$s = "some text";
			out::writeTo("content", "some direct text");*/
		}
		
		public function actionNum()
		{
			echo "3333";
		}
		
		public $defaultAction = "Num";
	}
